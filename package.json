    "config": {
        "forge": {
            "packagerConfig": {
                "ignore": [
                    "src",
                    "public_html/scss",
                    "nbproject",
                    "testfiles",
                    ".gitignore",
                    "gulpfile.js"
                ],
                "asar": true,
                "prune": true
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "electron-start"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
